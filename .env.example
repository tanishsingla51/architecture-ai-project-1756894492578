# Application Configuration
PORT=8080
CORS_ORIGIN=http://localhost:3000

# Database URL (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/claudedb?schema=public"

# JWT Configuration
JWT_SECRET="a_very_secret_and_strong_key_for_jwt"
JWT_EXPIRES_IN="7d"

# LLM Provider API Key (e.g., Anthropic, OpenAI)
# This is used by the llmService to make requests to the actual AI model.
LLM_API_KEY="YOUR_LLM_API_KEY_HERE"
